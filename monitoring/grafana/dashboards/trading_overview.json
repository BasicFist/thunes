{
  "dashboard": {
    "title": "THUNES Trading Overview",
    "tags": ["thunes", "trading", "phase13"],
    "timezone": "utc",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Daily PnL Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "thunes_daily_pnl_used",
            "legendFormat": "PnL Usage (fraction of limit)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "PnL Usage", "logBase": 1, "max": 1.5, "min": 0}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.8], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "PnL Limit Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "thresholds": [
          {"value": 0.8, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 2,
        "title": "Open Positions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "thunes_open_positions",
            "legendFormat": "Open Positions",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 2, "color": "yellow"},
                {"value": 3, "color": "red"}
              ]
            },
            "mappings": [],
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "auto",
          "textMode": "value_and_name",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        }
      },
      {
        "id": 3,
        "title": "Kill-Switch Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "thunes_kill_switch_active",
            "legendFormat": "Kill-Switch (0=OK, 1=ACTIVE)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "red"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "OK"},
              {"type": "value", "value": "1", "text": "ACTIVE"}
            ],
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "auto",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 4,
        "title": "Circuit Breaker State",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "thunes_circuit_breaker_state",
            "legendFormat": "{{breaker_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.4, "color": "yellow"},
                {"value": 0.9, "color": "red"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "CLOSED"},
              {"type": "value", "value": "0.5", "text": "HALF-OPEN"},
              {"type": "value", "value": "1", "text": "OPEN"}
            ],
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "auto",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 5,
        "title": "Order Latency (p50/p95/p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(thunes_order_latency_ms_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(thunes_order_latency_ms_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(thunes_order_latency_ms_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency", "logBase": 1, "min": 0}
        ],
        "thresholds": [
          {"value": 500, "colorMode": "critical", "op": "gt", "fill": false, "line": true}
        ]
      },
      {
        "id": 6,
        "title": "WebSocket Health",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "thunes_ws_connected",
            "legendFormat": "{{symbol}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "green"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DISCONNECTED"},
              {"type": "value", "value": "1", "text": "CONNECTED"}
            ],
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "horizontal",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 7,
        "title": "Orders Placed (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 4},
        "targets": [
          {
            "expr": "sum(increase(thunes_orders_placed_total[24h]))",
            "legendFormat": "Total Orders (24h)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "auto",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 8,
        "title": "WebSocket Message Gap",
        "type": "graph",
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(thunes_ws_gap_seconds_bucket[5m]))",
            "legendFormat": "p95 Gap (seconds)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Gap Duration", "logBase": 1, "min": 0, "max": 60}
        ],
        "thresholds": [
          {"value": 30, "colorMode": "critical", "op": "gt", "fill": false, "line": true}
        ]
      },
      {
        "id": 9,
        "title": "MCP Server Health",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "lab_mcp_server_health",
            "legendFormat": "{{server_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "green"},
                {"value": 1.5, "color": "yellow"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"},
              {"type": "value", "value": "2", "text": "NOT_CFG"}
            ],
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        }
      },
      {
        "id": 10,
        "title": "Worktree Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "lab_worktree_status",
            "legendFormat": "{{worktree_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "green"},
                {"value": 1.5, "color": "yellow"},
                {"value": 2.5, "color": "red"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "COMPLETE"},
              {"type": "value", "value": "1", "text": "WORKING"},
              {"type": "value", "value": "2", "text": "TESTING"},
              {"type": "value", "value": "3", "text": "BLOCKED"}
            ],
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "horizontal",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 11,
        "title": "Worktree Test Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "lab_worktree_test_status",
            "legendFormat": "{{worktree_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "dark-gray"},
                {"value": 0.5, "color": "green"},
                {"value": 1.5, "color": "red"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "UNKNOWN"},
              {"type": "value", "value": "1", "text": "PASSING"},
              {"type": "value", "value": "2", "text": "FAILING"}
            ],
            "unit": "short"
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "orientation": "horizontal",
          "textMode": "value_and_name"
        }
      }
    ]
  }
}
